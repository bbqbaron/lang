(defmodule lang.hello)

(defnative println : (-> String Unit)
  (java.io.PrintStream/println java.lang.System/out))

(deftype StringWrapper
  (| [:wrap String]
     [:crap Int]))

(def greeting
     [:wrap "Hello world! This is in a completely separate def."])

(defn main [_]
  (match greeting
         [:crap _] (println "crap")
         [:wrap text] (println text)))
