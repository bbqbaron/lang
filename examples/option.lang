(defmodule examples.option
  (:import [lang.io :as io]
           [lang.option :as option]))


(def val
  [:option/some "Planet"])


(defn id
  [x]
  x)


(defn apply
  [f x]
  (f x))


(defn main
  [argv : (Array String)]
  (io/println (option/default ((fn [x] x) val) "Hello"))
  (io/println (option/default (option/map (fn [x] "World") val) "fail")))
